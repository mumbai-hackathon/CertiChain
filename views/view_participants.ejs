<!DOCTYPE html>
<html>
<head>
	<title>View</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
	<link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.7.0/css/all.css'>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<style>	
		button[type="button"], button[type="submit"]{
			border-radius: 50px;
			margin-left: 4vh;
			cursor: pointer;
		}
		/* .table-striped>tbody>tr:nth-child(odd)>td, 
		.table-striped>tbody>tr:nth-child(odd)>th {
    		background-color: gray; 
		} */
	</style>

</head>
<body>
	<div>
		<% include navbar.ejs ;%>
	</div>

		<div class="container" style="margin-top:12vh;">
		    <h2>Recruiter List</h2>
		  <p></p>            
		  <table class="table">
		    <thead>
		      <tr>
		        <th>Person Name</th>
		        <th>Year</th>
		        <th>Branch</th>
		        <th>Rank</th>
		        <th>View/Accept/Reject</th>
		      </tr>
		    </thead>
		    <!-- <% JSON.stringify(jsonObj) %> -->
		    <% jsonObj.forEach(function(result){ %>
		    <tbody>
		      <tr>
		        <td><%= result.fname %> <%= result.lname %></td>
		        <td><%= result.year %></td>
		        <td><%= result.branch %></td>
		        <td><%= result.rank %></td>

		        <td class="row">
					<button class="btn btn-success" type="button">Accept</button>
					<button class="btn btn-danger" type="button">Reject</button>
				</td>
		      </tr>
		      
		    </tbody>

		    <% }); %>
		  </table>

		  <div>
		  	<form method="post" action="/faculty/generateCertificate">
		  		<input type="hidden" name="EventId" value="<%= eId %>">
		  		<button class="btn btn-success" type="submit">Generate Certificate</button>
		  	</form>
		  </div>

		</div>

</body>

<script>
	

</script>

</html>